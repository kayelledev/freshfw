.access_management_block
  = form_for @role, :html => {:multipart => true} do |f|
    .role_permission_block
      = f.error_messages
      = f.label :name
      = f.text_field :name, :class => 'text'
      = render 'permissions_form', :f => f
    = f.submit t('shoppe.submit'), :class => 'button green fixed-button'