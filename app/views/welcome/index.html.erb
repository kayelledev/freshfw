<!-- Carousel 
	================================================== -->
<header class="intro-header" style="background-image: url('http://gofourwalls.com/wp-content/uploads/2015/03/Pinnacle-small-space-lightened.jpg')">
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <img class="first-slide" src="<%= Rails.application.config.carousel1s3path %>" alt="First slide">
      <div class="container">
        <div class="carousel-caption">
          <h1>Tired of furniture shopping?</h1>
          <h2>Shop from the comfort of your home</h2>
          <p><a class="btn-landing" href="<%= products_url %>">Shop now</a></p>
        </div>
      </div>
    </div>
	
    <div class="item">
      <img class="fill" src="<%= Rails.application.config.carousel2s3path %>" alt="Second slide">
      <div class="container">
        <div class="carousel-caption">
          
	
	  	<!-- Begin MailChimp Signup Form -->
	  	<link href="//cdn-images.mailchimp.com/embedcode/classic-081711.css" rel="stylesheet" type="text/css">
	  	<style type="text/css">
	  		#mc_embed_signup{background:#fff; clear:left; font:14px Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif; }
	  		/* Add your own MailChimp form style overrides in your site stylesheet or in this style block.
	  		   We recommend moving this block and the preceding CSS link to the HEAD of your HTML file. */
	  	</style>
